  <select id="selectShopCartByCusid" parameterType="int" resultMap="bgdmap">
select a.id as cartid,b.id,b.indeximg,b.goodsName,b.price,b.ckPrice,a.num,b.price*a.num as money,c.id as tasteid,c.typeName as tastename,d.id as spackageid,d.typeName as packname from b_cart a left join b_goods b on a.goodsId=b.id

left join s_tastetype c on a.tasteId=c.id left join s_packagetype d on a.packageId=d.id

where a.cusid=#{id}
  </select>
  
  
