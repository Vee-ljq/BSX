 SELECT a.*,b.goodsName,b.price,c.typeName tastename,d.typeName packagename FROM `b_cart` a LEFT JOIN b_goods b on a.goodsId=b.id LEFT JOIN s_tastetype c on a.tasteId=c.id LEFT JOIN s_packagetype  d on a.packageId =d.id WHERE a.ID=#{cartId} 
