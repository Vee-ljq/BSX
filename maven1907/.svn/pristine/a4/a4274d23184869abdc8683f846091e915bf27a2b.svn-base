	<script src="<%=path %>/js/layui/js/layui2.js" charset="utf-8"></script>
	<script type="text/javascript">
$(function(){


  var wlno=$("#wlno").val();
  var logisticsum=$("#logisticsum").val();
 
   $.ajax({
            url:"<%=path%>/goodskd/selectwuliu.action",
            data: {
            wlno:wlno,
            logisticsum:logisticsum,
            },
            type: "post",
            dataType: "json",
            success: function(result){
              for(var i=0;i<result.Traces.length;i++){
              $("#wlline ul").append("<li class='layui-timeline-item'><i class='layui-icon layui-timeline-axis'>♕</i><div class='layui-timeline-content layui-text'><h3 class='layui-timeline-title'>"+ result.Traces[i].AcceptTime+"</h3></div>"+result.Traces[i].AcceptStation
    +"</div></li>");
              }

            }
        });
    });
    </script>

