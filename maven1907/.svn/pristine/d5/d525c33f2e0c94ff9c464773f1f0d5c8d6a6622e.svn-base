  <select id="selectAddressByCusid" parameterType="map" resultMap="admap">
  SELECT a.id,a.isdefault,a.cusid,a.realName,a.proviceId,a.cityId,a.areaId,a.address,b.mobile,c.cityName as sheng,d.cityName as shi,e.cityName as xian FROM `b_address` a left join b_customer b on a.cusId=b.id left join area_data c on a.proviceId=c.codeid left join area_data d on a.cityId=d.codeid left join area_data e on a.areaId=e.codeid 
  <where>
  	a.cusId=#{id}
  	<if test="addressid!=null">
  		and a.id = #{addressid}
  	</if>
  </where>
  
