package com.ry.dto;

import com.ry.pojo.SOrganization;
import com.ry.pojo.SUser;

public class OrgDto extends SOrganization {

	private SOrganization parents;
	
	private SUser user;
	

	public SUser getUser() {
		return user;
	}

	public void setUser(SUser user) {
		this.user = user;
	}

	public SOrganization getParents() {
		return parents;
	}

	public void setParents(SOrganization parents) {
		this.parents = parents;
	}
	
}
