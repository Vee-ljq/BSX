  <select id="selectnews" parameterType="com.ry.dto.BmessageDto" resultMap="map1">
		  SELECT * FROM `b_message` a
		left join 
		s_messagetype b
		on a.typeId=b.id
		where typeId>3
		 and a.content like "%${content}%"
  </select>
  
