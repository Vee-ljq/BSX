   <select id="selectsy" parameterType="com.ry.dto.BmessageDto" resultMap="ma">
		SELECT a.ID,typeId,a.titile,a.content,c.id as b,c.type,c.newsname FROM `b_message` a
		LEFT JOIN 
		s_messagetype c
		on a.typeId=c.id
		where a.id=#{id}
  </select>
  
