  <select id="selectCuidGoodsView" resultMap="cgvmap">
  SELECT a.goodsid,a.cuid,b.goodsname,b.goodsinfo,b.price,b.indexImg FROM `b_goodsview` a left join b_goods b on a.goodsid = b.id
  </select>

	<select id="goodsFlow" resultMap="cgvmap">
		select a.typeName,count(b.id) as goodsFlow,browsingtime from s_goodstype a
		left join s_goodstype b on a.id=b.pid
		left join b_goods c on c.typeId=b.id
		left join b_goodsview d on d.goodsId=c.id
		where year(browsingtime)=2020 and month(browsingtime)=1
		group by a.id
	</select>
