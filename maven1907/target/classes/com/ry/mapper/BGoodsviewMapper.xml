    <!-- 七日内的浏览商品 -->
	<select id="selectGoodsViews" parameterType="com.ry.dto.BGoodsViewDto" resultMap="map1">
		SELECT a.*,b.* 
FROM b_goodsview  a
LEFT JOIN
 (SELECT * FROM b_goods) b   ON a.goodsid=b.id
 WHERE (cuid=#{cuid} AND DATE_SUB(CURDATE(), INTERVAL 7 DAY) &lt;= DATE(browsingtime))
	</select>

  <select id="selectByTime" parameterType="com.ry.dto.BGoodsViewDto" resultMap="map1">
  	select a.*,b.* 
  	from b_goodsview a
  	left join (select * from b_goods) b on a.goodsid=b.id
  	where (cuid=#{cuid} and browsingtime=#{browsingtime})
  </select>

